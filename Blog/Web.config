  <connectionStrings>
  <add name="BlogEntities" connectionString="metadata=res://*/Entities.Blog.csdl|res://*/Entities.Blog.ssdl|res://*/Entities.Blog.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=Blog;initial catalog=Blog;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" /></connectionStrings>
  <!--
    Para obtener una descripción de los cambios de web.config, vea http://go.microsoft.com/fwlink/?LinkId=235367.

    Los siguientes atributos se pueden establecer en la etiqueta <httpRuntime>.
